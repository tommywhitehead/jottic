# Supabase Configuration
# Get these from your Supabase project dashboard
VITE_SUPABASE_URL=your_supabase_project_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# Liveblocks Configuration
# Get this from your Liveblocks dashboard
VITE_LIVEBLOCKS_SECRET_KEY=your_liveblocks_secret_key

# OAuth Configuration
# For development: http://localhost:5173/auth/callback
# For production: https://yourdomain.com/auth/callback
VITE_OAUTH_REDIRECT_URL=http://localhost:5173/auth/callback

# Production Environment Variables (for Vercel deployment)
# These are the same as above but without VITE_ prefix for server-side usage
SUPABASE_URL=your_supabase_project_url
SUPABASE_ANON_KEY=your_supabase_anon_key
LIVEBLOCKS_SECRET_KEY=your_liveblocks_secret_key
